# EdgeMed Agent — Environment Variables
# Copy to .env and fill in your values

# Google Cloud
EDGEMED_PROJECT_ID=edgemedsoinar
EDGEMED_GOOGLE_CLOUD_PROJECT=edgemedsoinar

# Mode control
EDGEMED_DEMO_MODE_ENABLED=true
EDGEMED_PROD_MODE_ENABLED=false
EDGEMED_STORE_RAW_NOTES_PROD=false

# BigQuery
EDGEMED_BQ_DATASET=edgemed_analytics

# Rate limiting (DEMO mode)
EDGEMED_RATE_LIMIT_PER_MIN=30

# Local app
EDGEMED_CLOUD_API_URL=http://localhost:8080
EDGEMED_DEVICE_ID=local-dev-001
EDGEMED_MODE=prod

# Google API key (Gemini API) — used only for: chatbot and prescription-from-image. NOT for medical image interpretation.
# Do not commit the real key. Get it from https://aistudio.google.com/apikey
EDGEMED_GOOGLE_API_KEY=

# MedGemma (cloud — Vertex AI deployed endpoint)
# Step 1: Deploy MedGemma from Model Garden (publishers/google/models/medgemma)
# Step 2: Set the endpoint ID here after deployment. Use multimodal variant for medical image interpretation.
EDGEMED_MEDGEMMA_MODEL=publishers/google/models/medgemma
EDGEMED_MEDGEMMA_VERSION=medgemma-1.5-4b-it
EDGEMED_MEDGEMMA_ENDPOINT_ID=
EDGEMED_MEDGEMMA_REGION=us-central1

# MedGemma (local — HuggingFace, optional)
EDGEMED_USE_MEDGEMMA_LOCAL=false
EDGEMED_MEDGEMMA_MODEL_ID=google/medgemma-1.5-4b-it
EDGEMED_MEDGEMMA_DEVICE=cuda
